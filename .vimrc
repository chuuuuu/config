inoremap ( ()<Esc>i
inoremap () ()
inoremap " ""<Esc>i
inoremap ' ''<Esc>i
inoremap [ []<Esc>i
inoremap { {}<Esc>i

set term=xterm-256color
set ai
set background=dark
set cursorline
set enc=utf8
set hls
set mouse=a
set number
set ic
set expandtab
set tabstop=2
set shiftwidth=2
set wrap
set noeb
set autochdir
set scrolloff=3
set foldenable
set foldmethod=indent
set foldcolumn=1
set foldlevel=5
set shell=/bin/bash
set cursorline
set nocompatible
syntax on
